<div class="flex-space-between shell-tab">
  <div class= "flex-50 padding">
    <% element.star.to_i.times do %>
      <i class="fa fa-star yellow-star"></i><!--reference 'components/_reviewstars.scss'-->
      <% end %><!--end of r.star-->
    <% (5 - element.star.to_i).times do %>
      <i class="fa fa-star grey-star"></i><!--reference 'components/_reviewstars.scss'-->
   <% end %><!--end of 5-r.star-->
  </div><!--end of review-item-->
  <div class="flex-50 padding">
    <h6><b><%= truncate(element.consultation.consult_title, length: 20)%></b></h6>
    <h6><%= truncate(element.review_title, length: 20)%></h6>
  </div><!--end review-item comment-->
</div><!--flex-space-between shell-tab-->
