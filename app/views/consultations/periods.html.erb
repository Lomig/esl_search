<div class="content-grid element-index-area">
  <div class="element-index-header">
    <h1>hi from header</h1>
  </div><!--end of index-header-->
  <div class="element-index-panel dashboard-user-panel-links-shell shell-border-dark black-background white-text">
    <%= render "consultations/index_panel" %>
  </div><!--end of index-panel-->
  <div class="element-index-shell shell-border">
    <div style="background-color: white;">
      <!-- Dropdown appearing on all devices -->
      <div class="navbar-esl-item">
      </div>
      <div class="container-fluid">
        <div class="row">
          <% @consult_period_links.each do |cper_link| %>
            <div class="col-md-6">
              <%= link_to period_consultations_path(consult_period: cper_link) do %>
                <div class="white-background card1 center-text">
                  <h3><%= cper_link %></h3>
                </div><!--end white card1 center-text-->
              <% end %><!--end of link-->
            </div><!--col-md-6-->
          <% end %><!--end of cloc_link-->
        </div><!--row-->
      </div><!--container-->
    </div>
  </div><!--end of index-shell-->
  <div class="element-index-base">
    <h1>hellow from base</h1>
  </div><!--end of index-base-->
</div>
