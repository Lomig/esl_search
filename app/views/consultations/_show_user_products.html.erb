<!--this is full size for the user_products index-->
<div class="flex-space-between padding-5">
  <!--product data-->
  <div class="flex-20"><h6><%= truncate( element.consult_title, length: 45) %></h6></div>
  <div class="flex-20"><h6><%= truncate(element.consult_focus) %></h6></div>
  <div class="flex-20 center-text"><h6><%= number_to_currency(element[:consultfee], :unit => "â‚©")%></h6></div>
  <div class="flex-20 center-text"><%= link_to consultation_path(element) do %>
