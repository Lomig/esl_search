<div class="flex-space-between">
  <div class="flex-33">
  <div class="" style="background-image: url('<%= image_path avatar_url %>'); background-size: cover; height: 40px; width: 40px; border-radius: 50%; background-position: center;"></div>
  </div>
  <div class="flex-67">
    <h4><%= element.consult_title %></h4>
