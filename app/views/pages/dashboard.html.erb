<div class="content-grid dash-area">
  <!-- <h1>Pages#dashboard</h1> -->
  <div class="dashboard-user-panel-link-div">
    <div class="user-dashboard-profile-image-div">
      <%= image_tag "user-icon.png", class: "user-dashboard-profile-image-img" %>
    </div><!--end of user-dashboard-profile-image-div-->
      <ul class="dashboard-user-link-list">
        <li><%= link_to "new consult", new_consultation_path %></li>
        <li><%= link_to "about me", about_path %></li>
        <li><%= link_to "messages", dashboard_path %></li>
        <li><%= link_to "all consultations", consultations_path %></li>
      </ul><!--end of dashboard-user-link-list -->
  </div><!--end of dashbooard-user-link-div-->
  <div class="dashboard-user-info-div">
    <h6 class=""><%= current_user.fname %></h6>
    <h6 class=""><%= current_user.email %></h6>
  </div><!--end of dashboard-user-info-div-->
  <div class="dashboard-messages-div">
     <h3 class="dashboard-message-header">Messages</h3>
  </div><!--end of dashboard-messages-div-->
  <div class="dashboard-appointment-div">
    <h3 class="my-appointments-header">MyAppointments</h3>
    <h5>appointments</h5>
      <div class="appointments-div">
        <% current_user.appointments.each do |a| %>
          <div class="dashboard-user-appointments-div">
            <p><%= image_tag "user-icon.png", class: "user-dashboard-icon-image-sm" %> <%= a.start.strftime('%m/%d/%Y at %I:%M%p')%> </p>
          </div><!-- end of dashboard-user-appnt-->
        <% end %><!-- end of current_user.appointments.each do -->
      </div><!--end of appointments-div-->
  </div><!--end of dashboard-appointment-div-->
  <div class="dashboard-consultation-div">
    <h3 class="my-consultations-header">MyConsultations</h3>
    <h5>consultations</h5>
    <div class="consultations-div">
      <% current_user.consultations.each do |c| %>
        <div class="dashboard-user-consults-div">
        <%= c.consulttype %>
        <%= c.focus %>
        <%= number_to_currency(c.consultfee, unit: "â‚©") %>
        </div> <!-- end of dashboard-user-consults-div -->
      <% end %> <!-- end of current_user.consultations.each do-->
    </div><!--end of consult-div-->
  </div><!--end of dashboard-consultation-div-->
  <div class="dashboard-consult-reviews-div">
    <h3>RecentReviews</h3>
  </div><!--end dashboard-consult-reviews-div-->
  <div class="dashboard-client-div">
    <h3 class="dashboard-client-header">Contacts</h3>
      <% @consultations.each do |c|%>
        <%= c.created_at %>
      <% end %>
    <ul class="dashboard-client-list">
      <li><%= image_tag "user-icon.png", class: "user-dashboard-icon-image-sm" %></li>
      <li><%= image_tag "user-icon.png", class: "user-dashboard-icon-image-sm" %></li>
    </ul>
  </div><!--end of dashboard-client-div-->
</div><!--end of grid div-->
