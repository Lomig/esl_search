<!--minified version for dashboard-->
<div class="flex-space-between shell-tab">
  <div class="flex-33"><h4><%= element.user.fname %> </h4></div>
  <div class="flex-33"><h6><%=truncate( element.subject, length: 25)%></h6></div>
  <% if element.created_at.strftime('%b %d %y') == DateTime.now.strftime('%b %d %y')%>
    <div class="flex-33 center-text"><h6><b><%= element.created_at.strftime('%I:%M %p')%></b></h6></div>
  <% else %>
    <div class="flex-33 center-text"><h6><b><%= element.created_at.strftime('%m %d %Y')%></b></h6></div>
  <% end %><!--end of if statement; if today, then show time-->
</div><!--end of flexbox shell-tab-->
